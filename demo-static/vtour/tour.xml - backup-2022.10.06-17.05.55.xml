	<scene name="scene_bg1" title="bg1" onstart="" thumburl="panos/bg1.tiles/thumb.jpg" lat="" lng="" heading="">
		
		<control bouncinglimits="calc:image.cube ? true : false" />

		<view hlookat="0.0" vlookat="0.0" fovtype="MFOV" fov="120" maxpixelzoom="2.0" fovmin="70" fovmax="140" limitview="auto" />

		<preview url="panos/bg1.tiles/preview.jpg" />

		<image>
			<cube url="panos/bg1.tiles/pano_%s.jpg" />
		</image>

	</scene>

